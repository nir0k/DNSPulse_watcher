#=======================================================================================================
# DNS settings
#=======================================================================================================
# Path to file with list of DNS servers
DNS_RESOLVERPATH=dns_servers.csv
# polling rate without recusrion in number of checks per second
DNS_POLLING_RATE_NO_RECURSION=5
# polling rate with recusrion in number of checks per second
DNS_POLLING_RATE_RECURSION=2
# DNS answer timeout in seconds
DNS_TIMEOUT=1
# DNS protocol. Possible value: tcp, udp, udp4, udp6, tcp4, tcp6
DNS_PROTOCOL=udp
# Add checking host in metric label. Possible value: true or false
DNS_CHECK_HOST_INCLUDE=false

#=======================================================================================================
# Prometheus
#=======================================================================================================
# Prometheus remote write url. example: http://prometheus:8428/api/v1/write
PROM_URL=http://prometheus:8428/api/v1/write
# Prometheus metric name
PROM_METRIC=dns_resolve
# Prometheus authentication. false or true. If true, values PROM_USER and PROM_PASS are required
PROM_AUTH=false
# Prometheus username
PROM_USER=testuser
# Prometheus password
PROM_PASS=testuser
# Count retries for post data in prometheus
PROM_RETRIES=2
# Single send to prometheus
PROM_SEND_SINGLE=true

#=======================================================================================================
# Watcher config
#=======================================================================================================
# Path to log file
LOG_FILE=high_frequency_dns_mon.log
# Minimal severity level for logging. Possible values: debug, info, warning, error, fatal (default: warning)
LOG_LEVEL=debug
# Interval check changes in config in minutes
CONF_CHECK_INTERVAL=5
# Timeseries buffer size for sent to prometheus
BUFFER_SIZE=2
# Watcher location
WATCHER_LOCATION=K37
# Watcher security zone
WATCHER_SECURITYZONE=PROD-INF-MNM
# Allow or not dublicate record
DUBLICATE_ALLOW=false